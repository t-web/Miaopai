public class Picture {
	
	public int category;
	public String img;
	public String title;
	
	public Picture(String path){
		this.img = path;
	}
	public Picture(String path,String title){
		this.img = path;
		this.title = title;
	}
	public Picture(int category, String img, String title) {
		super();
		this.category = category;
		this.img = img;
		this.title = title;
	}
}
