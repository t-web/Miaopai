<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/launch_back">

    <ImageView 
        android:id="@+id/img_launch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:src="@drawable/launcher_page_text"
        android:layout_marginTop="@dimen/launch_top"
        />
    <ImageView 
        android:id="@+id/img_launch_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/cat_in_launch"
        android:layout_alignParentBottom="true"
        android:scaleType="fitEnd"/>

</RelativeLayout>
